# MediReach Environment Configuration
# Copy this file to .env.local and fill in your actual values

# Google Maps API (Optional - currently using MapLibre)
# Get your API key from: https://console.cloud.google.com/google/maps-apis
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Backend API URL (for future backend integration)
NEXT_PUBLIC_API_URL=http://localhost:5000/api

# MongoDB Connection (for future backend)
MONGODB_URI=mongodb://localhost:27017/medireach
# OR for MongoDB Atlas:
# MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/medireach?retryWrites=true&w=majority

# JWT Secret (for future authentication)
JWT_SECRET=your_super_secret_jwt_key_here_change_this_in_production

# Twilio Configuration (for SMS alerts - future)
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=+1234567890

# Firebase Configuration (alternative to JWT - future)
# NEXT_PUBLIC_FIREBASE_API_KEY=your_firebase_api_key
# NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
# NEXT_PUBLIC_FIREBASE_PROJECT_ID=your_project_id
# NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
# NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=123456789
# NEXT_PUBLIC_FIREBASE_APP_ID=1:123456789:web:abcdef

# Node Environment
NODE_ENV=development
